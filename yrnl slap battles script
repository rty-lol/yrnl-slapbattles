	Name = "Destroy UI",
	Callback = function()
    OrionLib:Destroy()
	end
})
local InfoServer = Home:AddSection({Name = "Info"})
CanYouFps = Home:AddLabel("Your Fps [ "..math.floor(workspace:GetRealPhysicsFPS()).." ]")
CheckSlap = Home:AddLabel("Check Slaps [ "..game.Players.LocalPlayer.leaderstats.Slaps.Value.." ]")
CheckHealth = Home:AddLabel("your Health [ "..game.Players.LocalPlayer.Character.Humanoid.Health.." ]")
CheckGlove = Home:AddLabel("your Glove [ "..game.Players.LocalPlayer.leaderstats.Glove.Value.." ]")
CheckCode = Home:AddLabel("Keypad Code [ "..tostring((#game.Players:GetPlayers()) * 25 + 1100 - 7).." ]")
Home:AddParagraph("Game's ID [ "..game.PlaceId.." ]","Server ID [ "..game.JobId.." ]")

AutoSetInfo = Home:AddToggle({
	Name = "Auto Set Info",
	Default = false,
	Callback = function(Value)
_G.AutoSetInfo = Value
while _G.AutoSetInfo do
CanYouFps:Set("Your Fps [ "..math.floor(workspace:GetRealPhysicsFPS()).." ]")
CheckSlap:Set("Check Slaps [ "..game.Players.LocalPlayer.leaderstats.Slaps.Value.." ]")
CheckHealth:Set("your Health [ "..game.Players.LocalPlayer.Character.Humanoid.Health.." ]")
CheckGlove:Set("your Glove [ "..game.Players.LocalPlayer.leaderstats.Glove.Value.." ]")
CheckCode:Set("Keypad Code [ "..tostring((#game.Players:GetPlayers()) * 25 + 1100 - 7).." ]")
task.wait()
end
	end    
})

Speed:AddSlider({
	Name = "WalkSpeed",
	Min = 20,
	Max = 200,
	Default = 20,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
Walkspeed = Value
	end    
})

Speed:AddTextbox({
	Name = "WalkSpeed",
	Default = "UserSpeed",
	TextDisappear = false,
	Callback = function(Value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
Walkspeed = Value
	end	  
})

Speed:AddToggle({
    Name = "Walkspeed Auto set",
	Default = false,
	Callback = function(Value)
		KeepWalkspeed = Value
            while KeepWalkspeed do
                if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") ~= nil and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= Walkspeed then
                    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Walkspeed
                end
task.wait()
            end
	end    
})

